@model IEnumerable<Project2.Models.Missions>
@{
    ViewBag.Title = "Mission";
}

<h2>**DO WE NEED THIS PAGE?*Select Mission</h2>


<div class="dropdown">
    <button class="btn btn-primary dropdown-toggle" type="button" data-toggle="dropdown">
        Select a Mission
        <span class="caret"></span>
    </button>
    <ul class="dropdown-menu">
        @foreach (var item in Model)
        {
            <li><a class="dropdown-item" href="@Url.Action("Cambodia","Missions")">@Html.DisplayFor(model => item.MissionName)</a></li>
        }

    </ul>
</div>